<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <title>Community page</title>
</head>
<body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Community page</h1>
    <div class="evtCardss">
        <% events.forEach( (evt) => { %>
            <div class="evtCard">
                    <a href="/users/<%= user._id %>/Community/<%= evt._id %>">
                    <%= evt.name %> 
                    </a>
                    <form class="Fbtn3"
                    action="/users/<%= user._id %>/Community/<%= evt._id %>?_method=Post" method="POST">
                    <button type="submit" class="btn3" id="registerBtn">Register</button>
                    </form>
            </div>
        <% }) %>
    </div>
</body>
</html>